                                android:layout_height="wrap_content"
                                android:text="LDPE"
                                />
                            <CheckBox
                                android:id="@+id/chk_pvc"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="PVC"
                                />
                            <CheckBox
                                android:id="@+id/chk_pet"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="PET"
                                />
                            <CheckBox
